{
  "name": "webview",
  "version": "1.0.0",
  "scripts": {
    "start": "npm run server",
    "server": "webpack-dev-server --config config/dev.js",
    "clean": "rm -rf dist",
    "prebuild": "npm run lint && npm run test",
    "build": "npm run clean && webpack --config config/prod.js",
    "test": "jest --coverage",
    "tslint": "tslint",
    "lint": "npm run tslint \"src/**/*.ts\"",
    "prepush": "npm run build"
  },
  "dependencies": {
    "echarts": "^3.5.3",
    "typescript": "^2.2.2",
    "webpack": "^2.4.1",
    "webpack-dev-server": "^2.4.4",
    "webpack-merge": "^4.1.0"
  },
  "devDependencies": {
    "@types/echarts": "0.0.6",
    "@types/jest": "^19.2.2",
    "@types/webpack": "^2.2.15",
    "autoprefixer": "^6.7.7",
    "chalk": "^1.1.3",
    "css-loader": "^0.28.0",
    "html-webpack-plugin": "^2.28.0",
    "husky": "^0.13.3",
    "jest": "^19.0.2",
    "moment": "^2.18.1",
    "node-sass": "^4.5.2",
    "postcss-loader": "^1.3.3",
    "sass-loader": "^6.0.3",
    "style-loader": "^0.16.1",
    "ts-jest": "^19.0.10",
    "ts-loader": "^2.0.3",
    "tslint": "^5.1.0",
    "tslint-microsoft-contrib": "^4.0.1",
    "webpack-dev-server": "^2.4.4"
  },
  "jest": {
    "transform": {
      ".(ts|js)": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testResultsProcessor": "<rootDir>/node_modules/ts-jest/coverageprocessor.js",
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|js)$",
    "coverageDirectory": "coverage",
    "moduleFileExtensions": [
      "ts",
      "js"
    ]
  }
}
